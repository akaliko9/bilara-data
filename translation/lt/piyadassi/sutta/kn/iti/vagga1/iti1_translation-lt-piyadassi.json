{
  "iti1:0.1": "Taip bylota ",
  "iti1:0.2": "Vienetų knyga ",
  "iti1:0.3": "Pirmasis skyrius ",
  "iti1:0.4": "Godumas ",
  "iti1:1.1": "Aš girdėjau, kad taip tikrai bylojo Palaiminasis, taip bylojo _Arahantas_: ",
  "iti1:2.1": "„Vienuoliai, atsikratykite vienos būsenos, ",
  "iti1:2.2": "ir aš jums užtikrinu nebesugrįžtančiojo būvio (pasiekimą). ",
  "iti1:2.3": "Kokios vienos būsenos? ",
  "iti1:2.4": "Godumo, vienuoliai, atsikratykite (šios) vienos būsenos, ",
  "iti1:2.5": "ir aš jums užtikrinu nebesugrįžtančiojo būvio (pasiekimą). ",
  "iti1:2.6": "Apie tai Palaimintasis kalbėjo. ",
  "iti1:2.7": "Taip pasakyta apie tai: ",
  "iti1:3.1": "„Godumo, dėl kurio gobšaujančios ",
  "iti1:3.2": "būtybės keliauja į kančių pasaulį, "
}